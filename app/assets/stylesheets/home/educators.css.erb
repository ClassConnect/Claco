.whitebox{
	padding:10px;
	margin-bottom:10px;

  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;

	-moz-box-shadow:0 1px 3px rgba(34,25,25,0.3);
  -webkit-box-shadow:0 1px 3px rgba(34,25,25,0.3);
  box-shadow: 0 1px 3px rgba(34,25,25,0.3);

  background: #fff;
  width:600px;
  margin:20px auto 0 auto;
}

.whitebox .headnounce{
  font-size:18px;
  color:#333;
  font-weight:bolder;
  padding:12px 0 10px 10px;
  border-bottom:1px solid #EAEAEA;
}

.whitebox .listitem{
  padding:13px 0 13px 10px;
  border-bottom:1px solid #EAEAEA;
}

.whitebox .listitem .thumbster{
  float:left;
  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;
}

.whitebox .listitem .infowrap {
  float:left;
  margin-left:14px;
  width:430px;
}

.whitebox .listitem .bigtitle{
  font-size:16px;
  font-weight:bolder;
}

.whitebox .listitem .bigtitle .uname{
  color:#888 !important;
  font-size:13px !important;
  font-weight:normal !important;
  text-decoration: none !important;
}

.whitebox .listitem .biotxt{
  font-size:13px;
  color:#666;
  font-style: italic;
}

.whitebox .listitem .medata {
  margin-top:6px;
}

.whitebox .listitem .medata span{
  color:#777;
  font-size:12px;
}

.whitebox .listitem .medata span img{
  margin-top:-4px;
  margin-right:3px;
  opacity: 0.50;
  filter: alpha(opacity=50);
}
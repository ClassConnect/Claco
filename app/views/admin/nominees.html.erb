<table>
<tr>
<td>From</td>
<td>Email</td>
<td>Timestamp</td>
<td>Status</td>
</tr>
<% @nominees.each do |nom| %>
<tr>
<td><%= nom.from %></td>
<td><%= nom.email %></td>
<td><%= time_ago_in_words(Time.at(nom.timestamp).to_datetime) %> ago</td>
<td><% if nom.status == 0 %>
		<%= link_to("Approve", nom_approve_path(nom)) %> |
		<%= link_to("Deny", nom_deny_path(nom)) %>
	<% else %>
		<%= nom.status == 1 ? "Approved" : "Denied" %>
	<% end %></td>
</tr>
<% end %>
</table>
<% begin %>
	<%= paginate(@nominees) %>
	<%= page_entries_info(@nominees).html_safe %>
<% rescue %>
<% end %>
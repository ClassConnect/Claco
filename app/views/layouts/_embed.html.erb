<% if @binder.format == 1 %>
	<% if @croc %>

		<iframe id="crocFrame" style="width: 700px; height: 600px;" src="<%= @croc_url %>" ></iframe>

	<% end %>

	<%= link_to "Download #{@binder.title}", named_binder_route(@binder, "download")%>	

<% end %>

<% if @binder.format == 2 %>
	
	<% if @binder.current_version.embed %>

		<%= @binder.current_version.data.html_safe %>

	<% else %>

		<%= image_tag @binder.current_version.imgfile.contentview.url %>
		<%= link_to @binder.title, @binder.current_version.data %>

	<% end %>

<% end %>

<br /><br /><br />
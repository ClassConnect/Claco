<% if !@binder.current_version.get_html.empty? %>
	
	<%= @binder.current_version.get_html.html_safe %>

<% else %>

	<%= link_to(image_tag(@binder.current_version.img_contentview.url), @binder.current_version.data, :target => "_blank") %>

<% end %>

<br /><br />

<% if !@binder.current_version.embed %>
	<% if @binder.format == 1 %>

		<%= link_to "Download #{@binder.title}", named_binder_route(@binder, "download"), :target => "_blank" %>	

	<% else %>

		<%= link_to @binder.title, @binder.current_version.data, :target => "_blank" %>

	<% end %>
<% end %>

<br /><br /><br />
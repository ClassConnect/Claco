<!DOCTYPE html>
<html>
	<head>
		<title><%= @title %></title>
		<%= stylesheet_link_tag    "strap", :media => "all" %>
		<%= stylesheet_link_tag    "application", :media => "all" %>

		<% #stylesheet_link_tag    "box", :media => "all" %>
		<% #stylesheet_link_tag    "buttons/buttons", :media => "all" %>

		<%= stylesheet_link_tag    "jqueryui", :media => "all" %>

		<%= yield :headcon %>
		<%= csrf_meta_tags unless response.cache_control[:public] %>

		<% if signed_in? && current_teacher.getting_started %>

			<%= stylesheet_link_tag    "gen/guiders", :media => "all" %>
		<% end %>






	</head>
	<body>
		        <!--[if lt IE 7]>
<p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
<![endif]-->
		<header>
			<%= render 'layouts/navigation' %>
		</header>

		<div data-pjax-container>
			<%= yield %>
		</div>





		<div class="container" style="margin-top:20px">
			<div style="clear:both"></div>

			<hr>

			<footer>
			<p>&copy; Claco 2012 &nbsp;&nbsp;&nbsp;<a href="/">Home</a> / <a href="/about">About</a> /  <a href="/unitedweteach">#UnitedWeTeach</a> / <a href="/about/team">Our Team</a> / <a href="#" onclick="olark('api.box.expand'); return false">Contact</a> <span style="float:right;font-size:11px"><a href="http://schoolspeedtest.org/" target="_blank"><%= image_tag("public/highway.jpg", :style => "margin-right:20px;opacity: 0.50;filter: alpha(opacity=50);") %></a>Claco&trade; &bull; <a href="/legal/tos" target="_blank">Terms of Service</a> &bull; <a href="/legal/privacy" target="_blank">Privacy Policy</a></span></p>
			</footer>
		</div>




		<%= javascript_include_tag "application" %>
		<% #javascript_include_tag "dropdown", :defer => "defer" %>
		<% if signed_in? && current_teacher.getting_started %>

			<%= javascript_include_tag "gen/guiders" %>
			<%= javascript_include_tag "gen/wizard" %>

		<% end %>
		<% if params[:controller] != "home" || signed_in? %>
		<%= yield :botcon %>
		<%= javascript_include_tag "jqueryui" %>
		<%= javascript_include_tag "gen/cookie" %>
		<%= javascript_include_tag "gen/sticky" %>
		<%= javascript_include_tag "gen/boxresize" %>
		<%= javascript_include_tag "facebox" %>
		<%= javascript_include_tag "tabs" %>
		<%= javascript_tag "$('a.pjax').pjax('[data-pjax-container]');$.pjax.defaults.timeout = 5000;$.pjax.defaults.maxCacheLength=0;" %>
		<%= javascript_include_tag "tooltip" %>

		<% end %>


		<% if params[:controller] == "home" &&  !signed_in? %>
			<%= javascript_include_tag "home/pub" %>
		<% end %>


		<%= javascript_tag "page = '#{params[:controller]}##{params[:action]}'; loggedin = #{signed_in?}" %>

		<script type="text/javascript">

		  var _gaq = _gaq || [];
		  _gaq.push(['_setAccount', 'UA-34134443-1']);
